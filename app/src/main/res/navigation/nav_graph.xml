            android:id="@+id/action_mainFragment_to_addDialogFrament"
            app:destination="@id/addDialogFrament" />
    </fragment>

    <dialog
        android:id="@+id/addDialogFrament"
        android:name="com.example.newcalendar.AddDialogFrament"
        android:label="AddDialogFrament" />

</navigation>